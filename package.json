{
  "name": "platform3",
  "version": "1.0.0",
  "description": "Platform3 Trading System",
  "main": "dist/index.js",
  "scripts": {
    "test:mock-server": "ts-node src/testing/mock/mock-websocket-server.ts",
    "test:bridge:with-server": "concurrently \"npm run test:mock-server\" \"sleep 2 && npm run test:bridge\"",
    "test:bridge": "ts-node src/testing/test-runner.ts",
    "test:performance": "ts-node src/testing/performance-test.ts",
    "test:latency": "ts-node src/testing/latency-framework/latency.test.ts",
    "build": "tsc",
    "dev": "nodemon",
    "test:integration": "ts-node src/testing/integration/indicator-ai.test.ts",
    "test:config": "./scripts/test-config-system.sh",
    "setup:dev": "./infrastructure/setup-dev-environment.sh",
    "validate:config": "npm run test:integration && npm run test:config",
    "validate:production": "./scripts/production-validation.sh",
    "test:simple": "ts-node src/testing/simple-test.ts",
    "test:msgpack": "ts-node src/testing/msgpack-test.ts",
    "test:direct": "ts-node src/testing/direct-msgpack-test.ts",
    "test:full-bridge": "ts-node src/testing/full-bridge-test.ts"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "ml-matrix": "^6.10.5",
    "ml-regression": "^5.0.0",
    "simple-statistics": "^7.8.3",
    "technicalindicators": "^3.1.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.10",
    "axios": "^1.9.0",
    "concurrently": "^8.2.2",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "pg": "^8.16.0",
    "redis": "^5.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
