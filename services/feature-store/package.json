{
  "name": "feature-store-service",
  "version": "1.0.0",
  "description": "AI Feature Store for High-Frequency Forex Trading Platform",
  "main": "dist/feature-serving-api.js",
  "scripts": {
    "start": "node dist/feature-serving-api.js",
    "dev": "ts-node-dev --respawn --transpile-only src/feature-serving-api.ts",
    "build": "tsc",
    "pipeline": "python src/feature-pipeline.py",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t feature-store-service .",
    "docker:run": "docker run -p 3001:3001 feature-store-service"
  },
  "keywords": [
    "forex",
    "trading",
    "feature-store",
    "machine-learning",
    "real-time",
    "scalping",
    "high-frequency"
  ],
  "author": "Trading Platform Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "ws": "^8.14.2",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "typescript": "^5.2.2",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "numpy": "^0.0.1",
    "talib": "^1.1.5",
    "pyyaml": "^4.2.2",
    "asyncio": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "@types/ws": "^8.5.8",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.7",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trading-platform/feature-store.git"
  },
  "bugs": {
    "url": "https://github.com/trading-platform/feature-store/issues"
  },
  "homepage": "https://github.com/trading-platform/feature-store#readme"
}